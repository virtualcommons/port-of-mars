{
  "name": "port-of-mars-server",
  "version": "t0.1.0",
  "description": "Port Of Mars digital version",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p .",
    "build:deploy": "yarn build && rm -rf /code/server/src /code/shared && mv lib/server/src /code/server/src && mv lib/shared /code/shared",
    "backupdb": "pg_dump -h db -U marsmadness port_of_mars > pom-$(date +%Y-%m-%d-%H.%M.%S).sql",
    "cli": "ts-node -r tsconfig-paths/register src/cli.ts",
    "loadtest": "ts-node -r tsconfig-paths/register src/loadtest.ts",
    "cli:prod": "node -r tsconfig-paths/register src/cli.js",
    "start:prod": "node -r tsconfig-paths/register src/index.js | tee -a /var/log/port-of-mars/index.log",
    "start": "ts-node-dev -r tsconfig-paths/register src/index.ts | tee -a /var/log/port-of-mars/index.log",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "dangerously-resetdb": "dropdb -U marsmadness -h db port_of_mars && createdb -U marsmadness -h db port_of_mars && yarn typeorm schema:sync && yarn load-fixtures",
    "migrate": "yarn typeorm migration:run",
    "repl": "yarn ts-node -r tsconfig-paths/register",
    "sql": "pgcli -U marsmadness -d port_of_mars -h db",
    "test": "NODE_ENV=test jest",
    "lint": "eslint --ext .ts .",
    "lintfix": "eslint --fix --ext .ts .",
    "load-fixtures": "fixtures ./fixtures --config ormconfig.json --require ts-node/register --require tsconfig-paths/register"
  },
  "author": "Center for Behavior, Institutions, and the Environment (https://cbie.asu.edu)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/virtualcommons/port-of-mars/issues"
  },
  "homepage": "https://github.com/virtualcommons/port-of-mars#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/commander": "^2.12.2",
    "@types/connect-redis": "^0.0.16",
    "@types/cookie": "^0.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.10",
    "@types/express-session": "^1.17.3",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.167",
    "@types/mailgun-js": "^0.22.11",
    "@types/method-override": "^0.0.31",
    "@types/node": "^14.14.20",
    "@types/node-schedule": "^1.3.1",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-mailgun-transport": "^1.4.2",
    "@types/passport": "^1.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/pino": "^6.3.5",
    "@types/shelljs": "^0.8.8",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "csvtojson": "^2.0.10",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "shelljs": "^0.8.3",
    "ts-jest": "^26.4.4",
    "ts-node": "9.1.1",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "typeorm-fixtures-cli": "^1.9.0",
    "typescript": "^4.1.3",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "@sentry/node": "~5.29.2",
    "@vrbo/pino-rotating-file": "^2.0.1",
    "async-mutex": "^0.2.6",
    "bcryptjs": "^2.4.3",
    "colyseus": "^0.13.7",
    "commander": "^6.2.1",
    "connect-redis": "^5.0.0",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.3.0",
    "csv-writer": "^1.6.0",
    "express": "^4.16.4",
    "express-session": "^1.17.0",
    "helmet": "^4.3.1",
    "jsondiffpatch": "^0.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mailgun-js": "^0.22.0",
    "mysql": "^2.14.1",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.17",
    "nodemailer-mailgun-transport": "^2.0.2",
    "passport": "^0.4.1",
    "passport-cas2": "^0.0.11",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "pino": "^6.10.0",
    "redis": "^3.1.1",
    "reflect-metadata": "^0.1.10",
    "tinyqueue": "^2.0.3",
    "typeorm": "^0.2.30",
    "uuid": "^8.3.2",
    "validator": "^13.5.2"
  }
}
